# CarMax-vAuto Vehicle Enrichment Bot - Environment Configuration
# Copy this file to .env and fill in your actual values

# Production settings
NODE_ENV=production
PORT=3000

# CarMax credentials (REQUIRED)
CARMAX_EMAIL=your-carmax-email@example.com
CARMAX_PASSWORD=your-carmax-password

# vAuto credentials (REQUIRED)
VAUTO_USERNAME=your-vauto-username
VAUTO_PASSWORD=your-vauto-password

# Puppeteer configuration for headless server deployment
PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium
PUPPETEER_HEADLESS=true
PUPPETEER_TIMEOUT=30000

# Optional: Custom data directory
DATA_DIR=/app/data

# Optional: Debug logging
DEBUG=false
